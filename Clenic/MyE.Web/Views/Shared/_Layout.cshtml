    @*<script src="~/js/promise-polyfill.js"></script>*@
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@9"></script>
    <script>
        function swalShowLoading() { Swal.showLoading(); }
        function swalShowError() {
            Swal.fire({
                icon: 'error',
                title: '¡Error!',
                text: 'Ocurrió un error desconocido.'
            });
        }
    </script>

    <!-- Dropify -->
