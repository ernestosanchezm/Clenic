    <script>
        $('.dropify').dropify({
            messages: {
                'default': '',
                'replace': 'Arrastra y suelta o haz click para reemplazar',
                'remove': 'Eliminar',
                'error': 'Ocurrió un error. Por favor, intente nuevamente.'
            },
            error: {
                'fileSize': 'El archivo es muy grande ({{ value }} como máximo).',
                'minWidth': 'El ancho de la imagen es demasiado pequeño ({{ value }}px como mínimo).',
