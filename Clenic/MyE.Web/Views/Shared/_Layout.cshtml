    <script>
        function initValidator(e) {
            $(e).validate({
                errorClass: 'text-danger',
                errorElement: 'small',
                errorPlacement: function (error, element) {
                    error.insertAfter(element);
                }
            });
        }
        $(document).ready(function () { $('.validate').each(function (i, e) { initValidator(e); }); });
    </script>

