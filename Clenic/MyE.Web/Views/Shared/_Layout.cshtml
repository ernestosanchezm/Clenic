                'fileExtension': 'El tipo de archivo no es válido (solo se permiten: {{ value }}).'
            }
        });
        function initDropify(id) {
            $('#' + id).dropify({
                messages: {
                    'default': '',
                    'replace': 'Arrastra y suelta o haz click para reemplazar',
                    'remove': 'Eliminar',
                    'error': 'Ocurrió un error. Por favor, intente nuevamente.'
